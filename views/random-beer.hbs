

<div class="beerContainer">
    <h1>Random Beer</h1>
    {{#each beerRandom}}
    {{#if this.image_url}}
        <img src="{{this.image_url}}" alt="{{this.name}}">
    {{/if}}
    <h2>{{this.name}}</h2>
    <p>{{this.tagline}}</p>
    <p>{{this.description}}</p>
    <p>{{this.food_pairing}}</p>
<p>{{this.brewers_tips}}</p>
    {{/each}}
    
</div>